{
  "name": "Gmail",
  "description": "Read Gmail emails",
  "entrypoint": "index.js",
  "plugin_params": ["credentials_path", "token_path","obj"],

  "functions": [
    {
      "name": "getEmails",
      "description": "Fetch recent Gmail messages based on query",
      "async": true,
      "requires_LLM": true,
      "output_format": [
        {
          "from": "Sender email",
          "subject": "Email subject",
          "date": "Email date"
        }
      ],
      "keywords": [
        "email",
        "gmail",
        "inbox",
        "mail",
        "message",
        "messages",
        "from",
        "subject",
        "today",
        "this week"
      ],
      "requires": ["$INPUT"]
    }
  ]
}
